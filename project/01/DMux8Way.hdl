/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux4Way(in=in, sel=sel[0..1], a=a0, b=b0, c=c0, d=d0);
    DMux(in=in, sel=sel[2], a=a0, b=e);
    DMux(in=in, sel=sel[2], a=b0, b=f);
    DMux(in=in, sel=sel[2], a=c0, b=g);
    DMux(in=in, sel=sel[2], a=d0, b=h);
}